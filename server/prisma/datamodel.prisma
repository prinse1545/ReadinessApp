type User {
  id: ID! @id
  email: String! @unique
  name: String!
  password: String!
}

type Day {
  id: ID! @id
  userId: String!
  hoursOfSleep: Float!
  sleepQuality: Int!
  trainingDay: Boolean!
  fatigueLevel: Int!
  soreness: [sorenessDay!] @scalarList(strategy: RELATION)
  mentalStress: Int!
}

type sorenessDay {
  id: ID! @id
  bodyPart: String!
  sorenessLevel: Int!
}
